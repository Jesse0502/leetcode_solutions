    "class Solution:\n",
    "    def hammingWeight(self, n: int) -> int:\n",
    "        stripZeros = str(bin(n)).lstrip(\"0\")\n",
    "        output = 0\n",
    "        for s in stripZeros:\n",
    "            if s == \"1\":\n",
    "                output += 1\n",
    "        return output\n",
